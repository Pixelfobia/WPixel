{
  "name": "wpixel",
  "version": "1.0.0",
  "description": "WPixel theme packages",
  "main": "main.js",
  "scripts": {
    "prod": "cross-env NODE_ENV=production webpack --mode production --progress",
    "dev": "cross-env NODE_ENV=development webpack --watch --mode development --progress",
    "clean": "rm -rf build/*",
    "svg": "svgr -d src/js/icons src/icons",
    "lint": "eslint src/js/*",
    "lint:fix": "eslint --fix src/js/*",
    "lint-css": "stylelint 'src/sass/**/*.scss'",
    "lint-css:fix": "npm run lint-css -- --fix",
    "precommit": "npm run lint-css:fix && npm run lint:fix && npm run build"
  },
  "keywords": [
    "WordPress",
    "themes"
  ],
  "author": "Pixelfobia",
  "license": "MIT",
  "private": true,
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "bootstrap": "^5.1.3",
    "nano-css": "^5.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-react": "^7.22.3",
    "@svgr/cli": "^8.0.1",
    "@wordpress/base-styles": "^4.26.0",
    "@wordpress/block-editor": "^12.3.0",
    "@wordpress/blocks": "^12.12.0",
    "@wordpress/components": "^25.1.0",
    "@wordpress/compose": "^6.12.0",
    "@wordpress/data": "^9.5.0",
    "@wordpress/dependency-extraction-webpack-plugin": "^4.18.0",
    "@wordpress/element": "^5.12.0",
    "@wordpress/eslint-plugin": "^14.8.0",
    "@wordpress/hooks": "^3.35.0",
    "@wordpress/i18n": "^4.35.0",
    "@wordpress/icons": "^9.26.0",
    "@wordpress/server-side-render": "^4.12.0",
    "@wordpress/stylelint-config": "^21.18.0",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "eslint": "^8.42.0",
    "eslint-plugin-jsdoc": "^46.2.6",
    "file-loader": "^6.2.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.7.6",
    "node-sass": "^9.0.0",
    "sass": "^1.62.1",
    "sass-loader": "^13.3.1",
    "sass-mq": "^6.0.0",
    "stylelint": "^14.16.1",
    "stylelint-webpack-plugin": "^4.1.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.84.1",
    "webpack-cli": "^5.1.1",
    "wordpress": "^1.4.2"
  }
}
